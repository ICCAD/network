material SILICON :
   thermal conductivity     1.30e-4 ;
   volumetric heat capacity 1.628e-12 ;

microchannel 4rm :
   height 200 ;
   channel    length 100 ;
   wall       length 100 ;
   first wall length  100 ;
   last wall  length  100 ;
   wall material SILICON ;
   coolant flow rate 42 ;
   coolant heat transfer coefficient side 2.7132e-8 ,
                                      top 5.7132e-8 ,
                                   bottom 4.7132e-8 ;
   coolant volumetric heat capacity 4.176e-12 ;
   coolant incoming temperature 300.0 ;

die TOP_IC :
   source  2 SILICON ;
   layer  50 SILICON ;

die BOTTOM_IC :
   source  2 SILICON ;
   layer  50 SILICON ;

dimensions :
   chip length 10100, width 10100 ;
   cell length   100, width   100 ;

stack:
   die     TOP_DIE     TOP_IC    floorplan "./testcase_01/top.flp" ;
   channel CHANNEL_LAYER    ;
   die     BOTTOM_DIE       BOTTOM_IC  floorplan "./testcase_01/bottom.flp" ;

solver:
   steady ;
   initial temperature 300.0 ;

output:
   //T        (  TOP_DIE, 5000, 3000, "output11.txt",             final ) ;
   Tmap     (    TOP_DIE,             "./testcase_01/output_Top.txt",             final ) ;
   Tmap     (    BOTTOM_DIE,             "./testcase_01/output_Bottom.txt",             final ) ;
   //Tflp     (  TOP_DIE,             "output13.txt", minimum,    final ) ;
   //Tflpel   (   BOTTOM_DIE.Core1,        "output14.txt", average,    final ) ;
   //Tcoolant ( CHANNEL_LAYER,             "output15.txt", maximum,    final ) ;